gau target.com | httpx -status-code -threads 500 | grep 200 | grep "=" | awk '{print $1}'| grep -v ".jpg\|.jpeg\|.gif\|.css\|.tif\|.tiff\|.png|.ttf\|.woff\|.woff2\|.ico\|.pdf\|.woff2\|.svg\|.js" | qsreplace payload | anew | tee -a gauurls.txt;
gospider -s https://target.com -d 16 -a -w -c 50 | grep "=" | grep -oP '(http|https)://[^/"].*' | cut -d "]" -f1 | grep "target.com" | grep -v ".jpg\|.jpeg\|.gif\|.css\|.tif\|.tiff\|.png|.ttf\|.woff\|.woff2\|.ico\|.pdf\|.woff2\|.svg\|.js" | httpx -status-code -threads 500 | grep 200 | awk '{print $1}'| qsreplace payload | anew | tee -a gospiderurls.txt;
cat gauurls.txt gospiderurls.txt | sort -u | allurls.txt;
